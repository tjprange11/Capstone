@model IEnumerable<FoxAndHounds.Models.RequestOff>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Shift.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeAccepted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ManagerAccepted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Employee.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Shift.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeAccepted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ManagerAccepted)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
