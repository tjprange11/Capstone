@model IEnumerable<FoxAndHounds.Models.Schedule>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            <p>Start Date</p>
        </th>
        <th>
            <p>End Date</p>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Monday.Day.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sunday.Day.Date)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.Id })
            
        </td>
    </tr>
}

</table>
